{
  "name": "zentriqvision-upload",
  "version": "1.0.0",
  "description": "Lambda function for video upload",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "bundle": "esbuild dist/lambda/upload/index.js --bundle --platform=node --target=node18 --external:aws-sdk --external:@aws-sdk/client-dynamodb --external:@aws-sdk/util-dynamodb --external:@aws-sdk/client-s3 --external:@aws-sdk/s3-request-presigner --outfile=dist/bundle.js",
    "build:lambda": "npm run build && npm run bundle",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/s3-request-presigner": "^3.0.0",
    "@aws-sdk/util-dynamodb": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^18.0.0",
    "@types/uuid": "^9.0.0",
    "esbuild": "^0.25.8",
    "typescript": "^5.0.0"
  }
}
